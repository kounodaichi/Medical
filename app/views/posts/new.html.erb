<h1>患者予約管理</h1>
<%= form_with model: @post, local: true do |form| %>
  <div>
    <%= form.label :title, "患者名" %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :content, "記録内容" %>
    <%= form.text_field :content %>
  </div>
  <div>
    <%= form.submit "送信" %>
  </div>
  
  
<% end %>
